{
  "extends": "airbnb",
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts", ".scss", ".pug"]
      }
    }
  },
  "rules": {
    "max-len": [2, 100],
    "@typescript-eslint/no-unused-vars": 2,
    "no-console": 0,
    "import/extensions": 0,
    "quotes": 0,
    "semi": 0
  }
}
