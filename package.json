{
    "name": "sprint_4",
    "version": "1.0.0",
    "description": "",
    "engines": {
        "node": "16.17.1",
        "npm": "8.15.0"
    },
    "targets": {
        "default": {
            "engines": {
                "browsers": ">= 50%"
            }
        }
    },
    "scripts": {
        "dev": "webpack serve --mode=development",
        "build": "webpack --mode=production",
        "server": "node express.js",
        "start": "npm run build && node express.js",
        "eslint": "npx tsc --noEmit && npx eslint --config=./.eslintrc.json",
        "stylelint": "npx stylelint --fix ./src/**/*.css",
        "test": "./node_modules/.bin/mocha --require ts-node/register --require src/test.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run eslint && npm run stylelint && npm run test"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
