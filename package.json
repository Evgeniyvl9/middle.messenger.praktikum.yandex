        "express": "4.18.2",
        "handlebars": "4.7.7",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@types/chai": "4.3.4",
        "@types/jsdom": "21.1.0",
        "@types/mocha": "10.0.1",
        "@types/uuid": "9.0.0",
        "@typescript-eslint/eslint-plugin": "5.48.1",
        "@typescript-eslint/parser": "5.48.1",
        "chai": "4.3.7",
        "css-loader": "6.7.3",
        "eslint": "8.31.0",
        "fake-xml-http-request": "2.1.2",
